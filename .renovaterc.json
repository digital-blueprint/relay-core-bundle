{
  "extends": [
    "config:base",
    "group:allNonMajor",
    "schedule:weekly"
  ],
  "ignorePresets": [":prHourlyLimit2"],
  "rangeStrategy": "update-lockfile",
  "ignoreDeps": ["api-platform/core"],
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "symfony/"
      ],
      "allowedVersions": "<5"
    }
  ]
}