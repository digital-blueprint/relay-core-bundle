{
  "extends": [
    "config:base",
    "group:allNonMajor",
    "schedule:weekly"
  ],
  "ignorePresets": [":prHourlyLimit2"],
  "rangeStrategy": "update-lockfile",
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "symfony/"
      ],
      "allowedVersions": "<5"
    },
    {
      "matchPackageNames": ["php"],
      "allowedVersions": "<7.4"
    }
  ],
  "js": {
    "enabled": false
  }
}